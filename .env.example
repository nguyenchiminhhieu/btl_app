# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_anon_key_here

# Azure Speech Service
AZURE_SPEECH_KEY=your_azure_speech_key_here
AZURE_SPEECH_REGION=southeastasia

# OpenAI API
OPENAI_API_KEY=your_openai_api_key_here

# Google Speech API
GOOGLE_SPEECH_API_KEY=your_google_speech_api_key_here

# Backend URL (thay đổi theo IP của máy chạy backend)
# Kiểm tra IP: ipconfig (Windows) hoặc ifconfig (Mac/Linux)
# Format: http://YOUR_IP:3000
EXPO_PUBLIC_BACKEND_URL=http://192.168.1.XXX:3000
